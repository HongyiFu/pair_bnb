<h1>This is index page showing all users.</h1>

<table>
	<th>ID</th>
	<th>First Name</th>
	<th>Last Name</th>
	<th>Email</th>
	<th>Country</th>
	<% @users.each do |user| %>
		<tr>
			<td><%=user.id%></td>
			<td><%=user.first_name%></td>
			<td><%=user.last_name%></td>
			<td><%=user.email%></td>
			<td><%=user.country%></td>
		</tr>
	<% end %>
</table>